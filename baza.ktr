    <sql>WAITFOR DELAY '00:00:03';
USE Klienci;

IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'Miasta') AND type = N'U')
BEGIN
    CREATE TABLE Miasta
    (
        Miasto VARCHAR(19),
        kod_pocztowy VARCHAR(100),
        [Ilosc klientow] BIGINT
    )
END;

IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'Lata') AND type = N'U')
BEGIN
    CREATE TABLE Lata
    (
        [Zakres lat] VARCHAR(255),
        [Ilosc klientow] BIGINT
    )
END;

IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'Klienci') AND type = N'U')
BEGIN
CREATE TABLE Klienci
(
  id INT
, Imie VARCHAR(100)
, Nazwisko VARCHAR(100)
, Email VARCHAR(100)
, "Data urodzenia" DATETIME
, "Numer karty" VARCHAR(16)
)
END;

IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'Ksiazki') AND type = N'U')
BEGIN
CREATE TABLE Ksiazki
(
  tytul VARCHAR(255)
, kod VARCHAR(40)
, kategoria VARCHAR(30)
, cena FLOAT(53)
, dostepnosc VARCHAR(4)
)

END;

IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'Zamowienia') AND type = N'U')
BEGIN
CREATE TABLE Zamowienia
(
  nr_zamowienia VARCHAR(10)
, tytul VARCHAR(255)
, ilosc BIGINT
, kwota FLOAT(53)
, id_klienta BIGINT
)
END;

</sql>
